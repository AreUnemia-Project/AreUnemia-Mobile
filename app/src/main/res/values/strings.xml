    <string name="questionnaire">Questionnaire</string>

    <string name="question_one">Question 1</string>
    <string name="first_question">Do you often feel tired or fatigued, even after getting enough sleep?</string>

    <string name="question_two">Question 2</string>
    <string name="second_question">Do you notice that your skin tone, particularly on your face or palms, is paler than usual?</string>

    <string name="question_three">Question 3</string>
    <string name="third_question">Do you experience shortness of breath or rapid heartbeat, especially during physical activity?</string>

    <string name="question_four">Question 4</string>
    <string name="fourth_question"> Do you experience unusual or excessive bruising or bleeding, such as nosebleeds or bleeding gums?</string>

    <string name="question_five">Question 5</string>
    <string name="fifth_question">Do you have frequent episodes of dizziness or lightheadedness, particularly when standing up quickly?</string>

    <string name="question_six">Question 6</string>
    <string name="sixth_question">Do you often experience intense headaches, particularly ones that seem to come on suddenly or persist for long periods?</string>

    <string name="question_seven">Question 7</string>
    <string name="seventh_question">Have you noticed a significant change in your appetite, such as a decreased desire to eat or unusual cravings for non-food items?</string>

    <string name="question_eight">Question 8</string>
    <string name="eighth_question">Have you been feeling more irritable or moody than usual, without any obvious cause?</string>

    <string name="question_nine">Question 9</string>
    <string name="ninth_question">Do you experience numbness or tingling sensations, particularly in your hands or feet?</string>

    <string name="question_ten">Question 10</string>
    <string name="tenth_question">Have you noticed any changes in your nail beds, such as pale or spoon-shaped nails?</string>

